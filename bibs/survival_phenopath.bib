@ARTICLE{Naderi2007-mb,
  title       = "A gene-expression signature to predict survival in breast
                 cancer across independent data sets",
  author      = "Naderi, A and Teschendorff, A E and Barbosa-Morais, N L and
                 Pinder, S E and Green, A R and Powe, D G and Robertson, J F R
